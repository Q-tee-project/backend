# 데이터베이스 설정
DATABASE_URL=postgresql://username:password@localhost:5432/education_platform

# API 키 설정 (각자의 키를 사용)
# Math Service용 API 키
MATH_GEMINI_API_KEY=your_math_gemini_api_key_here

# English Service용 API 키  
ENGLISH_GEMINI_API_KEY=your_english_gemini_api_key_here

# 공통 Gemini API 키 (개별 키가 없을 때 사용)
GEMINI_API_KEY=your_common_gemini_api_key_here

# 서버 설정
HOST=0.0.0.0
PORT_MATH=8001
PORT_ENGLISH=8002
DEBUG=true

# CORS 설정
CORS_ORIGINS=["http://localhost:3000", "http://127.0.0.1:3000", "*"]

# Redis 설정 (Math Service - Celery용)
REDIS_URL=redis://localhost:6379/0

# Math Service 설정
MAX_PROBLEMS_PER_WORKSHEET=50
DEFAULT_DIFFICULTY=medium

# English Service 설정
MAX_QUESTIONS_PER_TEST=30
DEFAULT_TEST_TYPE=reading

# 개발자별 설정
DEVELOPER_NAME=your_name_here
DEVELOPER_EMAIL=your_email@example.com